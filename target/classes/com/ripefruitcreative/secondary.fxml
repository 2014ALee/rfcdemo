<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ripefruitcreative.SecondaryController">
    <children>
        <Label text="Secondary View" />
        <Button fx:id="secondaryButton" text="What kind of learner are you?" onAction="#startWIAGame" />
        <!-- Make sure the userData tag has the path to the required video on it. -->
        <Button fx:id="InSync" text="InSync" onAction="#vidSelect" userData="videos/InSync.mp4"/>
        <Button fx:id="dummy1" text="dummy1" onAction="#vidSelect" userData="videos/meme.mp4"/>
        <Button fx:id="dummy2" text="dummy2" onAction="#vidSelect" userData="videos/berd.mp4"/>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
