<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.media.Media?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.media.MediaPlayer?>

<VBox alignment="CENTER" spacing="20.0"
    xmlns="http://javafx.com/javafx/8.0.171"
    xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ripefruitcreative.QuizController">
    <children>
        <Label text="Video Results" />
        <HBox alignment="CENTER" spacing="20.0">
            <Label text="Different Sounds Heard" />
            <Label text="Different Sights Seen" />
        </HBox>
        <HBox alignment="CENTER" spacing="20.0">
            <Label fx:id= "gameLabelOne" text="${controller.audioStim}" />
            <Label fx:id= "gameLabelTwo" text="${controller.visualStim}" />
        </HBox>
        <Button fx:id="vidSelect" text="Back to video selection" onAction="#switchToVidSelect"/>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>

